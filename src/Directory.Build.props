<Project>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />

  <ItemGroup>
    <PackageReference Include="Meziantou.Analyzer" Version="1.0.566">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime;build;native;contentfiles;analyzers;buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <PropertyGroup>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(MSBuildThisFileDirectory)/NullableAttributes.cs" Link="Internals/NullableAttributes.cs" />
	  <Compile Include="$(MSBuildThisFileDirectory)/PlatformAttributes.cs" Link="Internals/PlatformAttributes.cs" />
  </ItemGroup>

</Project>